{
	"name": "vike-vue-template",
	"description": "Vike Vue Template",
	"author": "Mohamed Nesredin<https://github.com/Mohamed-Kaizen>",
	"license": "MIT",
	"homepage": "https://github.com/Mohamed-Kaizen/vike-vue-template#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Mohamed-Kaizen/vike-vue-template.git"
	},
	"bugs": {
		"url": "https://github.com/Mohamed-Kaizen/vike-vue-template/issues"
	},
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "tsx ./server",
		"dev:hmr": "node --import=tsx --watch-path=./server ./server",
		"build": "vite build",
		"commit": "cz commit",
		"i18n": "typesafe-i18n",
		"lint": "pnpm run lint_js && pnpm run  lint_prettier",
		"lint_fix": "prettier --write --list-different . && pnpm run lint_js --fix",
		"lint_js": "eslint --ext \".js,.ts,.vue\" .",
		"lint_prettier": "prettier --check .",
		"preview": "NODE_ENV=production tsx ./server"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.207",
		"@types/nprogress": "^0.2.3",
		"@types/serve-static": "^1.15.7",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"@unocss/reset": "^0.59.4",
		"@unpic/vue": "^0.0.50",
		"@vite-plugin-vercel/vike": "^6.0.1",
		"@vueuse/core": "^10.9.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-perfectionist": "^2.10.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-vue": "^9.25.0",
		"fast-glob": "^3.3.2",
		"nprogress": "^0.2.0",
		"ofetch": "^1.3.4",
		"prettier": "^3.2.5",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.4.5",
		"unocss": "^0.59.4",
		"unplugin-auto-import": "^0.17.5",
		"unplugin-vue-components": "^0.27.0",
		"vite-plugin-vercel": "^6.0.1"
	},
	"dependencies": {
		"@hattip/polyfills": "^0.0.45",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vue/compiler-sfc": "^3.4.27",
		"@vue/server-renderer": "^3.4.27",
		"cross-fetch": "^4.0.0",
		"h3": "~1.11.1",
		"serve-static": "^1.15.0",
		"tsx": "^4.9.3",
		"vike": "^0.4.171",
		"vike-vue": "^0.6.5",
		"vite": "^5.2.11",
		"vue": "^3.4.27"
	}
